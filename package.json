{
  "name": "grapesjs-style-filter",
  "version": "0.1.1",
  "description": "GrapesJS Style Filter",
  "main": "dist/grapesjs-style-filter.min.js",
  "scripts": {
    "lint": "eslint src",
    "patch": "npm version patch -m 'Bump v%s'",
    "build": "npm run patch && webpack --env.production",
    "start": "webpack-dev-server --open --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/artf/grapesjs-style-filter.git"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "style",
    "filter",
    "wysiwyg"
  ],
  "author": "Artur Arseniev",
  "license": "BSD-3-Clause",
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "babel-loader": "^8.0.4",
    "eslint": "^5.8.0",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
